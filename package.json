{
  "name": "food-maniac-server",
  "version": "0.7.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "BoServer:dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development SERVER_NAME=BoServer nodemon server/boServer.js --ignore 'server/static_dev/' --exec babel-node",
    "BoServer:dev:insertInitialData": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development SERVER_NAME=BoServer INSERT_INITIAL_DATA=true nodemon server/boServer.js --ignore 'server/static_dev/' --exec \"babel-node --presets es2015,stage-2\"",
    "BoServer:build:prod": "babel server -d distBoServer --copy-files",
    "BoServer:prod": "npm run BoServer:build:prod && cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production SERVER_NAME=BoServer nodemon distBoServer/boServer.js",
    "BoServer:prod:insertInitialData": "npm run BoServer:build:prod && cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production SERVER_NAME=BoServer INSERT_INITIAL_DATA=true nodemon distBoServer/boServer.js",
    "BoServer:prod:node": "cross-env NODE_ENV=production SERVER_NAME=BoServer node distBoServer/boServer.js",
    "BoServer:test": "cross-env  SERVER_NAME=BoServer NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register,css:test/mocha_ignore.js,png:test/mocha_ignore.js -d test/**/*.js",
    "BoServer:testVerbose": "./node_modules/.bin/mocha --compilers js:babel-register --reporter spec",
    "BoServer:lint": "eslint server",
    "lint": "eslint server"
  },
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "config": "^2.0.1",
    "cross-env": "^5.2.0",
    "cuid": "^2.1.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "file-stream-rotator": "0.3.1",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.0",
    "helmet": "^3.13.0",
    "if-env": "^1.0.4",
    "jimp": "^0.5.3",
    "jwks-rsa": "^1.3.0",
    "klaw": "^3.0.0",
    "mongoose": "^5.3.1",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "sanitize-html": "^1.19.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
